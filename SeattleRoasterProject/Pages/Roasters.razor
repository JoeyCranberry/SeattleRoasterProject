@page "/roasters"

@inject EnvironmentSettings envSett
@inject IConfiguration config

@if (curEnviroment != null)
{
	<RoasterListing @ref="roasterLisiting" CurEnviroment=curEnviroment.Value></RoasterListing>
}

@code {
	private RoasterListing? roasterLisiting;

	private EnvironmentSettings.Environment? curEnviroment;

	protected override void OnInitialized()
	{
		curEnviroment = envSett.GetEnvironment(config);
	}
}
